{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      ["m"],
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      [
        "ngx-metaui",
        "md",
        "m"
      ],
      "kebab-case"
    ]
  },
  "linterOptions": {
    "exclude": [
      "!**/*"
    ]
  }
}
